<%- include('./header') -%>

        <div class="col-md-12">
          <table class="table table-striped">
            <thead>
              <tr>
                <th scope="col" class="text-center">Address</th>
              </tr>
            </thead>
            <tbody>
            </tbody>
          </table>
        </div>
      </div>
    </div>
    <%- include('./footer') -%>
    <script>
      axios.get('/all').then((data) => {
        const list = data.data
        if (list.length > 0) {
          list.forEach((item) => {
            const html = `<tr><td><i class="fas fa-barcode"></i> 
              <a href="/address/${item}">
              ${item}
              </a>
              </td></tr>`
            $('tbody').append(html)
          })
        }
      })
    </script>
  </body>
</html>